rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure and for testing purposes only.
    // They allow any client to read and write to your entire database.
    // You should secure your database before deploying to production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
